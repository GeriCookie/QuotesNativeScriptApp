<Page xmlns="http://schemas.nativescript.org/tns.xsd" loaded="pageLoaded">
  <StackLayout>
  	<Label text="{{quoteText}}" textWrap="true" class="quote" />
  	<Label text="{{author}}" textWrap="true" class="author" />
  	
    <Button text="log in" tap="goToLogin" />
    <Button text="see all quotes" tap="goToQuotesList" />
  </StackLayout>
</Page>